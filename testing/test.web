server {
    listen 127.0.0.1:8080;
    server_name 127.0.0.1:8080 localhost:8080;
    error_page 404 /errors/404.html;
    client_max_body_size 10M;
    location / {
        root /home/mchetoui/cursus/webserv/testing;
        autoindex on;
        allow_methods GET;
    }
    location /resources {
        root /home/mchetoui/cursus/webserv/testing/resources;
        autoindex on;
        allow_methods GET;
    }
    location /uploads {
        root /home/mchetoui/cursus/webserv/testing/uploads;
        upload_enable on;
        upload_store /home/mchetoui/cursus/webserv/testing/uploads;
        autoindex on;
        allow_methods POST GET;
    }
    location /redirect {
        return 301 /;
    }
}