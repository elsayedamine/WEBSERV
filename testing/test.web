server {
    listen 127.0.0.1:8080;
    server_name example.com;
    error_page 404 /errors/404.html;
    client_max_body_size 10M;
    location /test {
        root /home/mchetoui/cursus/webserv/testing/test;
        index index.html
        autoindex off;
        allow_methods GET;
    }
    location / {
        root /home/mchetoui/cursus/webserv/testing;
        index index.html;
        autoindex off;
        allow_methods GET POST;
    }
    location /redirect {
        return 301 http://google.com;
    }
    location /uploads {
        root /home/mchetoui/cursus/webserv/testing/uploads;
        upload_enable on;
        upload_store /home/mchetoui/cursus/webserv/testing/uploads;
        allow_methods POST GET;
    }
}